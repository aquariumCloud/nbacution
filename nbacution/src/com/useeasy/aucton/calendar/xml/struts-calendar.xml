        </action>
        
        
        <action name="utAction_*" class="utAction" method="{1}">
           <result name="addUI">/WEB-INF/pages/useeasy/auction/calendar/ut/addUI.jsp</result>
            <result name="getList">/WEB-INF/pages/useeasy/auction/calendar/ut/getList.jsp</result>
            <result name="updateUI">/WEB-INF/pages/useeasy/auction/calendar/ut/updateUI.jsp</result>
      
        </action>
    </package>
	
</struts>
