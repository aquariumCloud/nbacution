<struts>

    <package name="calendar" namespace="/" extends="struts-base" >
    	
    	<action name="pmtingAction_*" class="pmtingAction"  method="{1}">
            <result name="addUI">/WEB-INF/pages/useeasy/auction/calendar/pmt/addUI.jsp</result>
            <result name="getList">/WEB-INF/pages/useeasy/auction/calendar/pmt/getList.jsp</result>
            <result name="updateUI">/WEB-INF/pages/useeasy/auction/calendar/pmt/updateUI.jsp</result>
         
        </action>
        
        
     
        <action name="pmtsqAction_*" class="pmtsqAction" method="{1}">
           <result name="getList">/WEB-INF/pages/useeasy/auction/shengqingting/getList.jsp</result>
           <result name="get">/WEB-INF/pages/useeasy/auction/shengqingting/get.jsp</result>
           <result name="tomonth">/WEB-INF/pages/useeasy/auction/shengqingting/getList.jsp</result>
           <result name="companysqGetList">/WEB-INF/pages/useeasy/auction/shengqingting/companysqGetList.jsp</result>   
