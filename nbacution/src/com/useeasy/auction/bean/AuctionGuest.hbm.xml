<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="com.useeasy.auction.bean.AuctionGuest" table="auction_guest_info" catalog="pmh" schema="dbo">
	 <cache usage="read-write"/>
		<id name="id" type="java.lang.Long" unsaved-value="0" >
			<column name="id_num" precision="22" scale="0" />
			<generator class="identity">
            </generator>
        </id>
        <property name="createTime" type="java.util.Date" update="true" insert="true" not-null="false">
            <column name="create_time" length="7" />
        </property>
        <property name="updateTime" type="java.util.Date" update="true" insert="true" not-null="false">
            <column name="update_time" length="7" />
        </property>
        <property name="deleteFlag" type="java.lang.String" update="true" insert="true" not-null="false">
            <column name="delete_flag" length="50" />
        </property>
        
        <property name="guestName" type="java.lang.String" update="true" insert="true" not-null="false">
            <column name="guest_name" length="50" />
        </property>
        <property name="guestIdentityCard" type="java.lang.String" update="true" insert="true" not-null="false">
            <column name="guest_identity_card" length="50" />
        </property>
        <property name="guestEmail" type="java.lang.String" update="true" insert="true" not-null="false">
            <column name="guest_email" length="100" />
        </property>
        <property name="guestPhone" type="java.lang.String" update="true" insert="true" not-null="false">
            <column name="guest_phone" length="50" />
        </property>
        <property name="guestTel" type="java.lang.String" update="true" insert="true" not-null="false">
            <column name="guest_tel" length="50" />
        </property>
         <property name="cardType" type="java.lang.String" update="true" insert="true" not-null="false">
            <column name="cardType" length="50" />
        </property>
     </class>
     
</hibernate-mapping>
